{
  "name": "super snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "html",
          "javascript",
          "javascriptreact"
        ],
        "path": "./snippets/html/html.json"
      },
      {
        "language": [
          "css",
          "scss",
          "sass"
        ],
        "path": "./snippets/css/css.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/javascript/javascript.json"
      }
    ]
  }
}
