{
  "function declaration": {
    "prefix": "fn",
    "body": "function ${1:name}(${2:arguments}) {\n\t$3\n}"
  },
  "arrow function": {
    "prefix": "afn",
    "body": "const ${1:name} = (${2:arguments}) => {\n\t$3\n}"
  },
  "import": {
    "prefix": "impmod",
    "body": "import { ${2:module} } from \"${1:source}\";"
  },
  "import react": {
    "prefix": "impr",
    "body": "import React from \"react\";"
  },
  "create component": {
    "prefix": "crc",
    "body": "import React from \"react\";\n\nconst $1 = () => {\n\treturn (\n\t\t<div>\n\t\t\t$2\n\t\t</div>\n\t);\n};\n\nexport default $3;"
  }
}
